void PARAMS_ResetToDefault(SENSOR_PARAMS *pParams, unsigned short type)
{
    *pParams = defaultParams;

    PARAMS_SetParameterValue(pParams, PARAM_INDEX_TYPE, type);

    if (PARAMS_IsAutomationSensor(pParams))     // 2.3.1
    {
    if (PARAMS_IsCentralizedSensor(pParams))
    {
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_DAYLIGHTFACTORCEN, PARAM_NUMBER_DAYLIGHTFACTORCEN);
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_ID1, PARAM_NUMBER_ID1);
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_ID2, PARAM_NUMBER_ID2);

        if (PARAMS_SensorHasNoTech(pParams))    // Centralized Daylight Sensor
        {
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_MAINGROUP, PARAM_NUMBER_MAINGROUP);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_SENDCONDITION, PARAM_NUMBER_SENDCONDITION);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_DEADBAND, PARAM_NUMBER_DEADBAND);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_TIMEBASE, PARAM_NUMBER_TIMEBASE);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_BANDWIDTH, PARAM_NUMBER_BANDWIDTH);
        }
        else
        {
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_HOUR, PARAM_NUMBER_HOUR);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_MINUTE, PARAM_NUMBER_MINUTE);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_SECOND, PARAM_NUMBER_SECOND);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_DETECTIONSCHEME, PARAM_NUMBER_DETECTIONSCHEME);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_SENSORSTATUS, PARAM_NUMBER_SENSORSTATUS);

            if (PARAMS_SensorHasAllTech(pParams))
            {
                // default values make sense
            }
            else if (PARAMS_SensorHasPIRandUS(pParams))
            {
                // default values make sense
            }
            else if (PARAMS_SensorHasPIR(pParams))
            {
                PARAMS_SetParameterValue(pParams, PARAM_INDEX_DETECTIONSCHEME, 1);
            }
            else if (PARAMS_SensorHasUS(pParams))
            {
                PARAMS_SetParameterValue(pParams, PARAM_INDEX_DETECTIONSCHEME, 2);
            }
            else if (PARAMS_SensorHasHF(pParams))
            {
                PARAMS_SetParameterValue(pParams, PARAM_INDEX_DETECTIONSCHEME, 5);
            }
        }
    }
    else if (PARAMS_IsStandaloneSensor(pParams))
    {
        if (PARAMS_SensorHasNoTech(pParams))
        {
        }
        else
        {
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_MINUTE, PARAM_NUMBER_MINUTE);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_SECOND, PARAM_NUMBER_SECOND);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_DAYLIGHTFACTOR, PARAM_NUMBER_DAYLIGHTFACTOR);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_NATURALLIGHTFACTOR, PARAM_NUMBER_NATURALLIGHTFACTOR);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_DAYLIGHTSETPOINT, PARAM_NUMBER_DAYLIGHTSETPOINT);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_PROVISIONOFLIGHT, PARAM_NUMBER_PROVISIONOFLIGHT);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_LIGHTREGULATION, PARAM_NUMBER_LIGHTREGULATION);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_MODE, PARAM_NUMBER_MODE);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_INITIAL, PARAM_NUMBER_INITIAL);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_MAINTAIN, PARAM_NUMBER_MAINTAIN);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_RETRIGGER, PARAM_NUMBER_RETRIGGER);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_ALERT, PARAM_NUMBER_ALERT);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_SENSORSTATUS, PARAM_NUMBER_SENSORSTATUS);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_CURRENTPOL, PARAM_NUMBER_CURRENTPOL);

            if (PARAMS_SensorHasAllTech(pParams))
            {
                // default values make sense
            }
            else if (PARAMS_SensorHasPIRandUS(pParams))
            {
                // default values make sense
            }
            else if (PARAMS_SensorHasPIR(pParams))
            {
                PARAMS_SetParameterValue(pParams, PARAM_INDEX_INITIAL, 1);
                PARAMS_SetParameterValue(pParams, PARAM_INDEX_MAINTAIN, 1);
                PARAMS_SetParameterValue(pParams, PARAM_INDEX_RETRIGGER, 1);
            }
            else if (PARAMS_SensorHasUS(pParams))
            {
                PARAMS_SetParameterValue(pParams, PARAM_INDEX_INITIAL, 2);
                PARAMS_SetParameterValue(pParams, PARAM_INDEX_MAINTAIN, 2);
                PARAMS_SetParameterValue(pParams, PARAM_INDEX_RETRIGGER, 2);
            }
            else if (PARAMS_SensorHasHF(pParams))
            {
                PARAMS_SetParameterValue(pParams, PARAM_INDEX_INITIAL, 5);
                PARAMS_SetParameterValue(pParams, PARAM_INDEX_MAINTAIN, 5);
                PARAMS_SetParameterValue(pParams, PARAM_INDEX_RETRIGGER, 5);
            }
        }

        if (PARAMS_IsZigbeeSensor(pParams))
        {
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_HOUR, PARAM_NUMBER_HOUR);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_ID1, PARAM_NUMBER_ID1);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_ID2, PARAM_NUMBER_ID2);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_ID3, PARAM_NUMBER_ID3);      // 2.3.1
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_ID4, PARAM_NUMBER_ID4);      // 2.3.1
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_BATTERYINFORMATION, PARAM_NUMBER_BATTERYINFORMATION);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_DBFIRMWAREVERSION, PARAM_NUMBER_DBFIRMWAREVERSION);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_DBFIRMWAREVERSION2, PARAM_NUMBER_DBFIRMWAREVERSION2);    // 2.3.1
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_DEVICEIDZIGBEE, PARAM_NUMBER_DEVICEIDZIGBEE);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_DEVICEIDZIGBEE2, PARAM_NUMBER_DEVICEIDZIGBEE2);      // 2.3.1
            if (!PARAMS_IsLineVoltageSensor(pParams))   // Battery Powered Zigbee Sensors, change default DeviceID Zigbee
            {
                PARAMS_SetParameterValue(pParams, PARAM_INDEX_DEVICEIDZIGBEE, 0x010C);
                PARAMS_SetParameterValue(pParams, PARAM_INDEX_DEVICEIDZIGBEE2, 0x010C);     // 2.3.1
            }
            else    // Line voltage Zigbee sensor
            {
                PARAMS_SetParameterNumber(pParams, PARAM_INDEX_LOADTYPE, PARAM_NUMBER_LOADTYPE);    // 2.2.0
                PARAMS_SetParameterNumber(pParams, PARAM_INDEX_POWERLINEFILTER, PARAM_NUMBER_POWERLINEFILTER);  // 2.2.0
            }
        }
        else if (PARAMS_IsDigitalSensor(pParams))
        {
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_REFERENT, PARAM_NUMBER_REFERENT);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_MAINGROUP, PARAM_NUMBER_MAINGROUP);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_HOUR, PARAM_NUMBER_HOUR);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_LOOPTYPE, PARAM_NUMBER_LOOPTYPE);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_ID1, PARAM_NUMBER_ID1);
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_ID2, PARAM_NUMBER_ID2);
        }
        else if (PARAMS_IsLineVoltageSensor(pParams))
        {
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_LOADTYPE, PARAM_NUMBER_LOADTYPE);    // 2.2.0
            PARAMS_SetParameterNumber(pParams, PARAM_INDEX_POWERLINEFILTER, PARAM_NUMBER_POWERLINEFILTER);  // 2.2.0
            if (!PARAMS_SensorHasHF(pParams))   // Only Line voltage PIR, US, Dual-tech sensors have Type2
            {
                PARAMS_SetParameterNumber(pParams, PARAM_INDEX_TYPE2, PARAM_NUMBER_TYPE2);      // 2.3.1
            }
        }
    }
    else
    {
    }

    if (PARAMS_SensorHasHF(pParams))    // then it's HF only
    {
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_HF_SENS, PARAM_NUMBER_HF_SENS);
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_STANDBYLEVEL, PARAM_NUMBER_STANDBYLEVEL);
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_STANDBYDELAY, PARAM_NUMBER_STANDBYDELAY);
    }

    if (PARAMS_SensorHasPIR(pParams))
    {
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_PIR_SENS, PARAM_NUMBER_PIR_SENS);
    }

    if (PARAMS_SensorHasUS(pParams))
    {
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_US_SENS, PARAM_NUMBER_US_SENS);
    }
    }   // 2.3.1
    // 2.3.1
    else if (PARAMS_IsLightController(pParams))
    {
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_HOUR, PARAM_NUMBER_HOUR);
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_MINUTE, PARAM_NUMBER_MINUTE);
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_SECOND, PARAM_NUMBER_SECOND);
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_DAYLIGHTFACTOR, PARAM_NUMBER_DAYLIGHTFACTOR);
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_NATURALLIGHTFACTOR, PARAM_NUMBER_NATURALLIGHTFACTOR);
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_DAYLIGHTSETPOINT, PARAM_NUMBER_DAYLIGHTSETPOINT);
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_PROVISIONOFLIGHT, PARAM_NUMBER_PROVISIONOFLIGHT);
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_LIGHTREGULATION, PARAM_NUMBER_LIGHTREGULATION);
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_YEAR, PARAM_NUMBER_YEAR);
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_MONTH, PARAM_NUMBER_MONTH);
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_DAY, PARAM_NUMBER_DAY);
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_SUMMERHOUR, PARAM_NUMBER_SUMMERHOUR);
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_STARTMONTH, PARAM_NUMBER_STARTMONTH);
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_STARTDAY, PARAM_NUMBER_STARTDAY);
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_ENDMONTH, PARAM_NUMBER_ENDMONTH);
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_ENDDAY, PARAM_NUMBER_ENDDAY);
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_COUNTRY, PARAM_NUMBER_COUNTRY);
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_LONGITUDE, PARAM_NUMBER_LONGITUDE);
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_LATITUDE, PARAM_NUMBER_LATITUDE);
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_CURRENTPOL, PARAM_NUMBER_CURRENTPOL);
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_GMTSIGN, PARAM_NUMBER_GMTSIGN);
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_GMTHOURS, PARAM_NUMBER_GMTHOURS);
        PARAMS_SetParameterNumber(pParams, PARAM_INDEX_GMTMINUTES, PARAM_NUMBER_GMTMINUTES);
        
        //PARAMS_SetParameterNumber(pParams, PARAM_INDEX_TYPE2, PARAM_NUMBER_TYPE2);    // Does Light Controller profile have Type2 or not?
    }
    else if (PARAMS_IsMeterSensor(pParams))
    {
    }
    else
    {
    }
    // 2.3.1
}

unsigned char PARAMS_IsParameterAvailable(SENSOR_PARAMS const *pParams, unsigned char Index)
{
    return (PARAMS_GetParameterNumber(pParams, Index) != PARAM_NUMBER_ERROR);
}

unsigned char PARAMS_ValidateParameterValue(unsigned char Index, unsigned short Value)
{
    if ((Index == PARAM_INDEX_HOUR) && (Value > 255))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_MINUTE) && (Value > 59))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_SECOND) && (Value > 59))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_HF_SENS) && (Value > 3))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_PIR_SENS) && (Value > 3))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_US_SENS) && (Value > 3))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_DAYLIGHTSETPOINT) && (Value > 255))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_DETECTIONSCHEME) && ((Value < 1) || (Value > 5)))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_INITIAL) && ((Value < 1) || (Value > 5)))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_MAINTAIN) && ((Value < 1) || (Value > 5)))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_RETRIGGER) && (Value > 5))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_MODE) && ((Value < 1) || (Value > 6)))    // 2.3.0
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_ALERT) && (Value > 3))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_STANDBYLEVEL) && (Value > 255))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_STANDBYDELAY) && (Value > 255))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_SENDCONDITION) && (Value > 3))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_DEADBAND) && ((Value < 1) || (Value > 100)))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_TIMEBASE) && ((Value < 1) || (Value > 59)))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_BANDWIDTH) && ((Value < 1) || (Value > 255)))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_LIGHTREGULATION) && (Value > 1))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_LOOPTYPE) && (Value > 1))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_PROVISIONOFLIGHT) && (Value > 255))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_YEAR) && (Value > 255))       // 2.3.1
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_MONTH) && ((Value == 0) || (Value > 12)))       // 2.3.1
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_DAY) && ((Value == 0) || (Value > 31)))         // 2.3.1
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_SUMMERHOUR) && (Value != 0) && (Value != 1) && (Value != 2) && (Value != 255))        // 2.3.1
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_STARTMONTH) && ((Value == 0) || (Value > 12)))       // 2.3.1
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_STARTDAY) && ((Value == 0) || (Value > 31)))       // 2.3.1
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_ENDMONTH) && ((Value == 0) || (Value > 12)))       // 2.3.1
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_ENDDAY) && ((Value == 0) || (Value > 31)))       // 2.3.1
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_COUNTRY) && (Value > 23))       // 2.3.1
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_LONGITUDE) && ((Value > 0xB4) && (Value < 0xFF4C)))       // 2.3.1
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_LATITUDE) && ((Value > 0x5A) && (Value < 0xFFA6)))       // 2.3.1
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_TYPE) && \
        (Value != PARAM_TYPE_LV_HF) && \
        (Value != PARAM_TYPE_LV_PIR) && \
        (Value != PARAM_TYPE_LV_US) && \
        (Value != PARAM_TYPE_LV_DUAL) && \
//        (Value != PARAM_TYPE_DGT_NONE) &&         // 1.0.24, comment out
        (Value != PARAM_TYPE_DGT_PIR) && \
        (Value != PARAM_TYPE_DGT_US) && \
        (Value != PARAM_TYPE_DGT_DUAL) && \
        (Value != PARAM_TYPE_CEN_DL) && \
        (Value != PARAM_TYPE_CEN_PIR) && \
        (Value != PARAM_TYPE_CEN_US) && \
        (Value != PARAM_TYPE_CEN_DUAL) && \
        (Value != PARAM_TYPE_ZB_PIR) && \
        (Value != PARAM_TYPE_ZB_LV_PIR) && \
        (Value != PARAM_TYPE_ZB_DUAL) && \
        (Value != PARAM_TYPE_ZB_LV_DUAL) && \
        (Value != PARAM_TYPE_LIGHT_CTRL))       // 2.3.1
    {
        return 0;
    }

    // Type2 value validation       // 2.3.1

    // Firmware Version value validation

    // ID1, ID2, ID3 and ID4 values validation      // 2.3.1

    if ((Index == PARAM_INDEX_REFERENT) && (Value > 0xAF))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_MAINGROUP) && (Value > 255))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_BATTERYINFORMATION) && (Value > 255))
    {
        return 0;
    }

    // Daughter board firmware Version values validation        // 2.3.1 (include Daughter firmware version 2)

    if ((Index == PARAM_INDEX_DEVICEIDZIGBEE) && (Value != 0x0103) && (Value != 0x010C) && (Value != 0x010D) && (Value != 0x010E))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_DEVICEIDZIGBEE2) && (Value != 0x0103) && (Value != 0x010C) && (Value != 0x010D) && (Value != 0x010E))     // 2.3.1
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_DAYLIGHTFACTOR) && ((Value < 1) || (Value > 255)))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_DAYLIGHTFACTORCEN) && ((Value < 1) || (Value > 255)))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_NATURALLIGHTFACTOR) && ((Value < 1) || (Value > 255)))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_DAYLIGHTLEVEL) && (Value > 19999))
    {
        return 0;
    }

    // Sensor Status value validation

    if ((Index == PARAM_INDEX_CURRENTPOL) && (Value > 255))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_LOADTYPE) && (Value > 6))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_POWERLINEFILTER) && (Value > 1))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_BUZZERFEEDBACK) && (Value > 1))
    {
        return 0;
    }

    if ((Index == PARAM_INDEX_FACTORYLIGHTFACTOR) && ((Value == 0) || (Value > 255)))       // 2.3.1
    {
        return 0;
    }

    return 1;
}

unsigned char PARAMS_ValidateDefaultFileValues(const SENSOR_PARAMS *pParams, unsigned char Index, unsigned short Value)
{
    if (PARAMS_SensorHasAllTech(pParams))
    {
        if ((Index == PARAM_INDEX_DETECTIONSCHEME) && ((Value < 1) || (Value > 5)))
        {
            return 0;
        }
        else if ((Index == PARAM_INDEX_INITIAL) && ((Value < 1) || (Value > 5)))
        {
            return 0;
        }
        else if ((Index == PARAM_INDEX_MAINTAIN) && ((Value < 1) || (Value > 5)))
        {
            return 0;
        }
        else if ((Index == PARAM_INDEX_RETRIGGER) && (Value > 5))
        {
            return 0;
        }
    }
    else if (PARAMS_SensorHasPIRandUS(pParams))
    {
        if ((Index == PARAM_INDEX_DETECTIONSCHEME) && ((Value < 1) || (Value > 4)))
        {
            return 0;
        }
        else if ((Index == PARAM_INDEX_INITIAL) && ((Value < 1) || (Value > 4)))
        {
            return 0;
        }
        else if ((Index == PARAM_INDEX_MAINTAIN) && ((Value < 1) || (Value > 4)))
        {
            return 0;
        }
        else if ((Index == PARAM_INDEX_RETRIGGER) && (Value > 4))
        {
            return 0;
        }
    }
    else if (PARAMS_SensorHasPIR(pParams))
    {
        if ((Index == PARAM_INDEX_DETECTIONSCHEME) && (Value != 1))
        {
            return 0;
        }
        else if ((Index == PARAM_INDEX_INITIAL) && (Value != 1))
        {
            return 0;
        }
        else if ((Index == PARAM_INDEX_MAINTAIN) && (Value != 1))
        {
            return 0;
        }
        else if ((Index == PARAM_INDEX_RETRIGGER) && (Value != 0) && (Value != 1))
        {
            return 0;
        }
    }
    else if (PARAMS_SensorHasUS(pParams))
    {
        if ((Index == PARAM_INDEX_DETECTIONSCHEME) && (Value != 2))
        {
            return 0;
        }
        else if ((Index == PARAM_INDEX_INITIAL) && (Value != 2))
        {
            return 0;
        }
        else if ((Index == PARAM_INDEX_MAINTAIN) && (Value != 2))
        {
            return 0;
        }
        else if ((Index == PARAM_INDEX_RETRIGGER) && (Value != 0) && (Value != 2))
        {
            return 0;
        }
    }
    else if (PARAMS_SensorHasHF(pParams))
    {
        if ((Index == PARAM_INDEX_DETECTIONSCHEME) && (Value != 5))
        {
            return 0;
        }
        else if ((Index == PARAM_INDEX_INITIAL) && (Value != 5))
        {
            return 0;
        }
        else if ((Index == PARAM_INDEX_MAINTAIN) && (Value != 5))
        {
            return 0;
        }
        else if ((Index == PARAM_INDEX_RETRIGGER) && (Value != 0) && (Value != 5))
        {
            return 0;
        }
    }

    return 1;
}

void PARAMS_InterpretStandbyLevel(SENSOR_PARAMS *pParams)
{
    unsigned short temp = PARAMS_GetParameterValue(pParams, PARAM_INDEX_STANDBYLEVEL);

    if (temp > valuesStandbyLevel[sizeof(valuesStandbyLevel)/sizeof(unsigned short)-1])   // if temp > maximum value, interpret as maximum value.
    {
        temp = valuesStandbyLevel[sizeof(valuesStandbyLevel)/sizeof(unsigned short)-1];
    }

    PARAMS_SetParameterValue(pParams, PARAM_INDEX_STANDBYLEVEL, temp);
}

void PARAMS_InterpretStandbyDelay(SENSOR_PARAMS *pParams)
{
    unsigned short temp = PARAMS_GetParameterValue(pParams, PARAM_INDEX_STANDBYDELAY);

    if (temp > valuesStandbyDelay[sizeof(valuesStandbyDelay)/sizeof(unsigned short)-2])     // Interpret 121 to 254 as 255
    {
        temp = valuesStandbyDelay[sizeof(valuesStandbyDelay)/sizeof(unsigned short)-1];
    }

    PARAMS_SetParameterValue(pParams, PARAM_INDEX_STANDBYDELAY, temp);
}
